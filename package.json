{
  "name": "@dollie/core",
  "version": "1.4.0",
  "description": "Core utilities for Dollie.js",
  "homepage": "https://github.com/dolliejs/dollie-core#readme",
  "author": {
    "name": "lenconda",
    "email": "i@lenconda.top",
    "url": "https://lenconda.top"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "bin",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "keywords": [
    "Webpack",
    "React.js",
    "Yeoman",
    "yeoman-generator"
  ],
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/diff": "^5.0.0",
    "@types/download": "^6.2.4",
    "@types/figlet": "^1.2.1",
    "@types/fs-extra": "^9.0.6",
    "@types/lodash": "^4.14.168",
    "@types/node": "^10.17.51",
    "@types/uuid": "^8.3.0",
    "@types/yeoman-environment": "^2.10.2",
    "@types/yeoman-generator": "^4.11.3",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "coveralls": "^3.0.5",
    "eslint": "^7.12.1",
    "eslint-config-alloy": "^3.10.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "md5-file": "^5.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "tsc": "^1.20150623.0",
    "typescript": "^4.1.3",
    "yeoman-assert": "^3.1.0",
    "yeoman-environment": "^2.10.3",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "diff": "^5.0.0",
    "download": "^8.0.0",
    "download-git-repo": "^3.0.2",
    "figlet": "^1.5.0",
    "fs-extra": "^9.1.0",
    "got": "^11.8.1",
    "lodash": "^4.17.20",
    "uuid": "^8.3.2",
    "yaml": "^1.10.0",
    "yeoman-generator": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "exit 0",
    "checksum": "node scripts/check.js",
    "clean:build": "rimraf lib",
    "build": "npm run clean:build && tsc"
  },
  "repository": "https://github.com/dolliejs/dollie-core.git",
  "license": "MIT"
}
